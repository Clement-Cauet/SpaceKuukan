<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_space">

    <fragment
        android:id="@+id/navigation_shop"
        android:name="com.spacekuukan.application.ui.FragmentShop"
        android:label="@string/title_shop"
        tools:layout="@layout/fragment_shop" />

    <fragment
        android:id="@+id/navigation_space"
        android:name="com.spacekuukan.application.ui.FragmentSpace"
        android:label="@string/title_space"
        tools:layout="@layout/fragment_space" />

    <fragment
        android:id="@+id/navigation_base"
        android:name="com.spacekuukan.application.ui.FragmentBase"
        android:label="@string/title_base"
